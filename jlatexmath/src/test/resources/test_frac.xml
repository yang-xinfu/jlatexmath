<?xml version='1.0'?>
<tests name="frac">
    <test>
        <id name="1"/>
        <code>
            <![CDATA[
               \frac{1}{2}
      ]]>
        </code>
        <expected success="true"/>
    </test>

    <test>
        <id name="2"/>
        <code>
            <![CDATA[
